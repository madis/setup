# Keep conf in .bashrc so the environment would be the same
# for login and ordinary shells
if [ -f ~/.bashrc ]; then
   source ~/.bashrc
fi
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

